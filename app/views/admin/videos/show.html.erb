<div class="page-header">
  <%= link_to admin_course_videos_path(@course), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_admin_course_video_path(@course, @admin_video), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar admin_video</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @admin_video.nome %></dd>

  <dt>Descricao:</dt>
  <dd><%= @admin_video.descricao %></dd>

  <dt>Link:</dt>
  <dd><%= @admin_video.link %></dd>

</dl>
