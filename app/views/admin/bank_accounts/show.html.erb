<div class="page-header">
  <%= link_to admin_administrator_bank_accounts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_admin_administrator_bank_account_path(@administrator, @admin_bank_account), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar admin_bank_account</h1>
</div>

<dl class="dl-horizontal">
  <dt>Numero:</dt>
  <dd><%= @admin_bank_account.numero %></dd>

  <dt>Agencia:</dt>
  <dd><%= @admin_bank_account.agencia %></dd>

  <dt>Banco:</dt>
  <dd><%= @admin_bank_account.banco %></dd>
</dl>
